<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
    <div></div>
    <div class="users flex">
    <% users.forEach(function(user){ %>
        <div>
            <h1><%= user.username%></h1>
            <h2><%= user.email%></h2>
            <img src="<%=user.image%>">
        </div>

   <% }) %>
    </div>
</body>
</html>